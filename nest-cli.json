{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/bot-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/bot-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/bot-gateway",
  "projects": {
    "bot-gateway": {
      "type": "application",
      "root": "apps/bot-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/bot-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bot-gateway/tsconfig.app.json"
      }
    },
    "bot-aws": {
      "type": "application",
      "root": "apps/bot-aws",
      "entryFile": "main",
      "sourceRoot": "apps/bot-aws/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bot-aws/tsconfig.app.json"
      }
    },
    "bot-auth": {
      "type": "application",
      "root": "apps/bot-auth",
      "entryFile": "main",
      "sourceRoot": "apps/bot-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bot-auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}